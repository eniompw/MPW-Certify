{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "app.py"
    }
  ],
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "installCommand": "pip install --no-cache-dir -r requirements.txt"
      }
    }
  ]
}
